generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@127.0.0.1:3306/api"
}

model User {
  id            String    @id @default(cuid())
  name          String?
  password      String
  email         String?   @unique
  role          String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}
