generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:newrootpassword@127.0.0.1:3306/api"
}

model User {
  id              Int               @id @default(autoincrement())
  email           String?           @unique
  name            String?
  surname         String?
  password        String?
  role            String
}
